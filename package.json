{
  "name": "astro-import-tree",
  "version": "0.1.3",
  "description": "A library to parse Astro page URL paths and import trees using AST",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "astro-import-tree": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
